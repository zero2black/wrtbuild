#!/bin/bash

z2b_api="https://api.github.com/repos/zero2black/wrtbuild/releases/latest" 

z2b_file_down="$(curl -s ${z2b_api} | grep "browser_download_url" | grep -oE "https.*.img.gz" | head -n 1)"

echo $z2b_file_down
cd && ls
wget $z2b_file_down

fw_backup
sysupgrade -v -n /root/*x86-64-generic-ext4-combined-efi.img.gz