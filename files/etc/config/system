
config system
	option hostname 'ImmortalWrt'
	option timezone 'WIB-7'
	option ttylogin '0'
	option log_size '6400'
	option urandom_seed '0'
	option zonename 'Asia/Jakarta'
	option log_proto 'udp'
	#option conloglevel '8'
	#option cronloglevel '5'
    option conloglevel '4'
    option cronloglevel '9'
	option log_file '/tmp/system.log'

config timeserver 'ntp'
	list server 'time1.apple.com'
	list server 'time1.google.com'
	list server 'time.cloudflare.com'
	list server 'pool.ntp.org'

config imm_init
	option ipv6 '1'

